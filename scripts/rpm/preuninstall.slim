INSTALL_PREFIX=$RPM_INSTALL_PREFIX1

if [ $1 = 0 ]; then
	/sbin/service dragent stop > /dev/null 2>&1
	/sbin/chkconfig --del dragent
fi

# Clean all generated Python bytecode
find $INSTALL_PREFIX/lib -iname "*.pyc" -delete > /dev/null 2>&1
find $INSTALL_PREFIX/lib -iname "*.pyo" -delete > /dev/null 2>&1
